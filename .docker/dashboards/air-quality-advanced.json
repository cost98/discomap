          "rawQuery": true,
          "rawSql": "SELECT \n  EXTRACT(HOUR FROM m.time)::integer as \"Hour\",\n  ROUND(AVG(CASE WHEN p.pollutant_name = 'NO2' THEN m.value END)::numeric, 2) as \"NO2\",\n  ROUND(AVG(CASE WHEN p.pollutant_name = 'PM10' THEN m.value END)::numeric, 2) as \"PM10\",\n  ROUND(AVG(CASE WHEN p.pollutant_name = 'PM2.5' THEN m.value END)::numeric, 2) as \"PM2.5\",\n  ROUND(AVG(CASE WHEN p.pollutant_name = 'O3' THEN m.value END)::numeric, 2) as \"O3\"\nFROM airquality.measurements m\nJOIN airquality.pollutants p ON m.pollutant_code = p.pollutant_code\nWHERE m.time > NOW() - INTERVAL '7 days'\n  AND p.pollutant_name IN ('NO2', 'PM10', 'PM2.5', 'O3')\nGROUP BY EXTRACT(HOUR FROM m.time)\nORDER BY \"Hour\";",
          "refId": "A"
        }
